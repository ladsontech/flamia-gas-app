      "platform": "narrow",
      "label": "Home screen of Flamia Gas"
    }
  ],
  "shortcuts": [
    {
      "name": "Order Gas",
      "url": "/order",
      "icons": [{ "src": "/lovable-uploads/icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Gas Refill",
      "url": "/refill",
      "icons": [{ "src": "/lovable-uploads/icon.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+gas",
      "url": "/order?type=%s"
    }
  ],
  "scope_extensions": [
    {
      "origin": "https://*.flamiagas.com"
    },
    {
      "origin": "https://*.lovableproject.com"
    }
  ],
  "file_handlers": [
    {
      "action": "/order",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      },
      "name": "Order from file",
      "icons": [
        {
          "src": "/lovable-uploads/icon.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share-target/",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "widgets": [
    {
      "name": "Quick Gas Order",
      "description": "Quickly order your favorite gas",
      "tag": "quick-order",
      "template": "quick-order-template",
      "ms_ac_template": "widgets/quick-order.json",
      "data": {
        "source": "/widgets/quick-order-data.json"
      },
      "screenshots": [
        {
          "src": "/lovable-uploads/icon.png",
          "sizes": "320x240",
          "label": "Quick Order Widget"
        }
      ],
      "icons": [
        {
          "src": "/lovable-uploads/icon.png",
          "sizes": "96x96"
        }
      ],
      "auth": false,
      "update": 86400
    }
  ],
  "capture_links": "existing-client-navigate",
  "scope": "/",
  "id": "flamia-gas-delivery-pwa",
  "lang": "en",
  "dir": "ltr",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "prefer-native",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "offline_enabled": true,
  "permissions_policy": {
    "geolocation": ["self"],
    "notifications": ["self"]
  },
  "permissions": ["notifications", "geolocation"],
  "install_experience": {
    "prefer_related_applications": false
  },
  "install_provider": "browser",
  "browser_action": {
    "default_icon": "/lovable-uploads/icon.png",
    "default_title": "Install Flamia Gas Delivery"
  },
  "app": {
    "background": {
      "scripts": ["/sw.js"]
    }
  },
  "apple_mobile_web_app_capable": "yes",
  "apple_mobile_web_app_status_bar_style": "black-translucent",
  "apple_mobile_web_app_title": "Flamia Gas"
}
