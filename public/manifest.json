
{
  "name": "Flamia - Gas Delivery Service",
  "short_name": "Flamia Gas",
  "description": "Quick and reliable gas delivery service in Uganda",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#FFFFFF",
  "theme_color": "#FF4D00",
  "orientation": "portrait",
  "categories": ["shopping", "utilities", "business"],
  "icons": [
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/home.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "platform": "narrow",
      "label": "Home screen of Flamia Gas"
    }
  ],
  "shortcuts": [
    {
      "name": "Order Gas",
      "url": "/order",
      "icons": [{ "src": "/lovable-uploads/icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Gas Refill",
      "url": "/refill",
      "icons": [{ "src": "/lovable-uploads/icon.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+gas",
      "url": "/order?type=%s"
    }
  ],
  "capture_links": "existing-client-navigate",
  "scope": "/",
  "id": "flamia-gas-delivery-pwa",
  "lang": "en",
  "dir": "ltr",
  "iarc_rating_id": "",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "offline_enabled": true,
  "permissions": ["notifications", "geolocation"]
}
