
{
  "name": "Flamia - Gas Delivery Service",
  "short_name": "Flamia Gas",
  "description": "Quick and reliable gas delivery service in Uganda",
  "start_url": "/?source=pwa",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#FFFFFF",
  "theme_color": "#00b341",
  "orientation": "portrait-primary",
  "categories": ["shopping", "utilities", "business"],
  "icons": [
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/lovable-uploads/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/home.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "platform": "narrow",
      "label": "Home screen of Flamia Gas"
    }
  ],
  "shortcuts": [
    {
      "name": "Order Gas",
      "url": "/order?source=shortcut",
      "icons": [{ "src": "/lovable-uploads/icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Gas Refill",
      "url": "/refill?source=shortcut", 
      "icons": [{ "src": "/lovable-uploads/icon.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+flamia",
      "url": "/order?type=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/order",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      },
      "name": "Order from file"
    }
  ],
  "share_target": {
    "action": "/share-target/",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "capture_links": "existing-client-navigate",
  "scope": "/",
  "id": "flamia-gas-delivery-pwa",
  "lang": "en",
  "dir": "ltr",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "auto",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  }
}
